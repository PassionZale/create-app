{
  "name": "react-admin-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "基于 React@18 的管理后台模板",
  "author": "@whouu/create-app",
  "scripts": {
    "dev": "npm run start:dev",
    "start:dev": "vite --config config/vite/serve.config.ts --mode dev",
    "start:beta": "vite --config config/vite/serve.config.ts --mode beta",
    "start:uat": "vite --config config/vite/serve.config.ts --mode uat",
    "start:prod": "vite --config config/vite/serve.config.ts --mode prod",
    "build:dev": "tsc --noEmit && vite build --config config/vite/build.config.ts --mode dev",
    "build:beta": "tsc --noEmit && vite build --config config/vite/build.config.ts --mode beta",
    "build:uat": "tsc --noEmit && vite build --config config/vite/build.config.ts --mode uat",
    "build:prod": "tsc --noEmit && vite build --config config/vite/build.config.ts --mode prod",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "4.8.1",
    "antd": "4.24.15",
    "axios": "^1.6.2",
    "pubsub-js": "^1.9.4",
    "react": "^18.2.0",
    "react-activation": "^0.12.4",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "tree-model": "^1.0.7",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.3",
    "@types/pubsub-js": "^1.8.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "consola": "^3.2.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "less": "^4.2.0",
    "less-vars-to-js": "^1.3.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-mock-dev-server": "^1.3.4",
    "vite-plugin-style-import": "^2.0.0"
  }
}
